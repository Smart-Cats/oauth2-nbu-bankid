{
    "name": "therezor/oauth2-nbu-bankid",
    "description": "National Bank of Ukraine OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Roman Nagirnyi",
            "email": "rnagirnyi@smartcats.pro",
            "homepage": "https://github.com/therezor"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authorisation",
        "bankid",
        "nbu"
    ],
    "require": {
        "league/oauth2-client": "^2.0",
        "jakeasmith/http_build_url": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.3.1",
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "TheRezor\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TheRezor\\OAuth2\\Client\\Test\\": "test/src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
